package utils;


/* given two integers, returns the minimum (signed comparison) */
func min(a : Int, b : Int) -> Int {
    if (a <= b) {
        return a;
    }
    else {
        return b;
    }
}


/* given two floats, returns the minimum (signed comparison) */
func min(a : Float, b : Float) -> Float {
    if (a <= b) {
        return a;
    }
    else {
        return b;
    }
}
